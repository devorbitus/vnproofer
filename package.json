{
  "name": "vnproofer",
  "version": "1.0.11",
  "description": "Command line Visual Novel Proofreading tool",
  "main": "src/vnproofer.mjs",
  "scripts": {
    "dev": "ts-node src/vnproofer.ts",
    "start": "ts-node src/vnproofer.ts",
    "test": "jest",
    "patch": "git diff --no-ext-diff --quiet --exit-code && npm version --no-git-tag-version patch && npx genversion --es6 src/version.ts",
    "patchc": "git commit -a -m 'patch version increment' && git push && git push --tag"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devorbitus/vnproofer.git"
  },
  "keywords": [
    "command-line",
    "renpy",
    "grammar",
    "gramma",
    "cspell"
  ],
  "author": "Chris Gruel | cgruel@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devorbitus/vnproofer/issues"
  },
  "homepage": "https://github.com/devorbitus/vnproofer#readme",
  "devDependencies": {
    "@types/cli-progress": "^3.9.1",
    "@types/node-fetch": "^2.5.10",
    "@types/prompts": "^2.0.12",
    "@types/shelljs": "^0.8.8",
    "@types/update-notifier": "^5.0.0",
    "genversion": "^2.3.1",
    "jest": "^26.6.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/node": "^15.3.0",
    "@types/yargs": "^15.0.13",
    "cli-progress": "^3.9.0",
    "escape-string-regexp": "^2.0.0",
    "insert-line": "^1.1.0",
    "kleur": "^3.0.3",
    "line-number": "^0.1.0",
    "node-fetch": "^2.6.1",
    "parse-diff": "^0.8.1",
    "prompts": "^2.4.1",
    "read-pkg": "^6.0.0",
    "shelljs": "^0.8.4",
    "update-notifier": "^5.1.0",
    "yargs": "^17.0.1"
  },
  "type": "module",
  "bin": {
    "vnproofer": "dist/vnproofer.js",
    "vnp": "dist/vnproofer.js"
  }
}
