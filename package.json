{
  "name": "vnproofer",
  "version": "1.0.8",
  "description": "Command line Visual Novel Proofreading tool",
  "main": "src/vnproofer.mjs",
  "scripts": {
    "start": "node src/vnproofer.mjs",
    "test": "jest",
    "patch": "git diff --no-ext-diff --quiet --exit-code && npm version --no-git-tag-version patch && npx genversion --es6 src/version.mjs && git commit -a -m 'patch version increment' && git push && git push --tag"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devorbitus/vnproofer.git"
  },
  "keywords": [
    "command-line",
    "renpy",
    "grammar",
    "gramma",
    "cspell"
  ],
  "author": "Chris Gruel | cgruel@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devorbitus/vnproofer/issues"
  },
  "homepage": "https://github.com/devorbitus/vnproofer#readme",
  "devDependencies": {
    "genversion": "^2.3.1",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cli-progress": "^3.9.0",
    "escape-string-regexp": "^2.0.0",
    "kleur": "^3.0.3",
    "line-number": "^0.1.0",
    "node-fetch": "^2.6.1",
    "parse-diff": "^0.8.1",
    "prompts": "^2.4.1",
    "read-pkg": "^6.0.0",
    "shelljs": "^0.8.4",
    "yargs": "^17.0.1"
  },
  "type": "module",
  "bin": {
    "vnproofer": "src/vnproofer.mjs"
  }
}
